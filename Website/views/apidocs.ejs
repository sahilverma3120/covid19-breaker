            <h4>QUERY PARAMETERS</h4>
            <table>
                <thead>
                <tr>
                    <th>Field</th>
                    <th>Type</th>
                    <th>Description</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                    <td>patientId</td>
                    <td>String</td>
                    <td>(optional) Unique ID number given for each reported patient</td>
                </tr>
                <tr>
                    <td>reportedOn</td>
                    <td>String</td>
                    <td>(optional) Case reported date</td>
                </tr>
                <tr>
                    <td>ageEstimate</td>
                    <td>String</td>
                    <td>
                        (optional) Approximate age of the patient
                    </td>
                </tr>
                <tr>
                    <td>gender</td>
                    <td>String</td>
                    <td>
                        (optional) a string to filter character by gender:<br>
                        male<br>
                        female
                    </td>
                </tr>
                <tr>
                    <td>city</td>
                    <td>String</td>
                    <td>(optional)The nearest city to which the case is reported</td>
                </tr>
                <tr>
                    <td>district</td>
                    <td>String</td>
                    <td>(optional) District in which the city is located</td>
                </tr>
				<tr>
                    <td>state</td>
                    <td>String</td>
                    <td>
                        (optional) State in which the district is located
                    </td>
                </tr>
				<tr>
                    <td>status</td>
                    <td>String</td>
                    <td>
                        (optional) Whether the patient is being hospitalized, or deceased or recovered
                    </td>
                </tr>
                </tbody>
            </table>
